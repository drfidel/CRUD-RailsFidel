<h1>Articles listing</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Text</th>
        <th>Link to</th>
    </tr>

   
    <%= @articles.each do |article| %>
    <tr>
        <td><%= article.title %><td>
        <td><%= article.text %><td>
        <td><%= link_to "Show", article_path(article) %><td>
    </tr>
    <% end %>
</table>