<h1>Articles listing</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Text</th>
        <th colspan="2">Links to</th>
    </tr>

   
    <%= @articles.each do |article| %>
    <tr>
        <td><%= article.title %><td>
        <td><%= article.text %><td>
        <td><%= link_to 'Show', article_path(article) %><td>
        <td><%= link_to 'Edit', edit_article_path(article) %><td>
    </tr>
    <% end %>
</table>
<%= link_to  'Create new article', new_article_path %>